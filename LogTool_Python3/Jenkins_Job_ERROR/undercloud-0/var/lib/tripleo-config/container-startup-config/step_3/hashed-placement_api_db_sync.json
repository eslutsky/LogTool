{
  "command": "/usr/bin/bootstrap_host_exec placement su placement -s /bin/bash -c '/usr/bin/placement-manage db sync'",
  "detach": false,
  "environment": {
    "TRIPLEO_DEPLOY_IDENTIFIER": "1583290741",
